class Stack:
    def __init__(self):
        self.arr = []
        self.length = 0

    def __repr__(self):
        return f'{self.arr}'

    def __len__(self):
        return len(self.arr)

    def add(self, data: [int, str]):
        self.arr.append(data)
        self.length += 1

    def peak(self):
        return self.arr[-1]

    def pop(self):
        self.length -= 1
        return self.arr.pop()

    def is_empthy(self):
        return True if len(self) <= 0 else False
